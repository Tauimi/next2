{
  "buildCommand": "prisma generate && next build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["fra1"],
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "PRISMA_ENGINES_CHECKSUM_IGNORE_MISSING": "1"
  }
} 